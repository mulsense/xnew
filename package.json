{
  "name": "xnew",
  "description": "JavaScript Library for Component-Oriented Programming.",
  "keywords": [
    "Component-Oriented Programming"
  ],
  "version": "2.4.3",
  "main": "dist/xnew.js",
  "module": "./dist/xnew.module.js",
  "exports": {
    ".": {
      "import": "./dist/xnew.module.js",
      "require": "./dist/xnew.js"
    },
    "./addons/*": {
      "import": "./dist/addons/*.module.js",
      "require": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mulsense/xnew/issues"
  },
  "homepage": "https://github.com/mulsense/xnew#readme",
  "scripts": {
    "test": "jest",
    "build": "npx rollup --config --bundleConfigAsCjs",
    "dev": "npx rollup --config --watch --bundleConfigAsCjs"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "matter-js": "^0.20.0",
    "pixi.js": "^8.9.1",
    "rollup": "^3.3.0",
    "three": "^0.175.0",
    "xnew": "^2.2.3"
  }
}
