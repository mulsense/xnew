{
  "name": "xnew",
  "description": "JavaScript Library for Component-Oriented Programming.",
  "keywords": [
    "Component-Oriented Programming"
  ],
  "version": "2.6.3",
  "main": "dist/xnew.js",
  "module": "./dist/xnew.mjs",
  "exports": {
    ".": {
      "import": "./dist/xnew.mjs",
      "require": "./dist/xnew.js"
    },
    "./addons/*": {
      "import": "./dist/addons/*.mjs",
      "require": "./dist/addons/*.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mulsense/xnew/issues"
  },
  "homepage": "https://github.com/mulsense/xnew#readme",
  "scripts": {
    "test": "jest",
    "build": "npx rollup --config --bundleConfigAsCjs",
    "dev": "npx rollup --config --watch --bundleConfigAsCjs",
    "cp": "node -e \"require('fs').cpSync('./dist', './examples/dist', {recursive: true})\"",
    "watch:cp": "onchange \"./dist/*\" -- npm run cp"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.17",
    "babel-jest": "^29.7.0",
    "cpx": "^1.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "onchange": "^7.1.0",
    "rollup": "^3.3.0",
    "ts-jest": "^29.3.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "xnew": "^2.6.3"
  }
}
